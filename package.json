{
  "name": "neurotic",
  "author": "James Smith",
  "version": "0.0.5",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/neurotic",
  "description": "A JavaScript neural network.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/neurotic"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@swc/core": "^1.5.6",
    "lively-cli": "^2.0.65",
    "watchful-cli": "^1.7.56"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "watchful": "watchful -m --transpiler=swc --source-directory=./src --lib-directory=./lib --wait=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug",
    "native-clean": "rm -rf ./target neurotic.node",
    "native-napi-debug": "napi build",
    "native-napi": "napi build --release",
    "native-build-debug": "npm run native-clean && npm run native-napi-debug",
    "native-build": "npm run native-clean && npm run native-napi"
  },
  "main": "./lib/index.js"
}
